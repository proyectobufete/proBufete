{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tipo de asuntos</h1>

    <table>
        <tbody>
            <tr>
                <th>Id tipoasunto</th>
                <td>{{ tipoasunto.idTipoasunto }}</td>
            </tr>
            <tr>
                <th>Asunto</th>
                <td>{{ tipoasunto.asunto }}</td>
            </tr>
            <tr>
                <th>Estado asunto</th>
                <td>{% if tipoasunto.estadoAsunto %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipoasuntos_index') }}">Regresar a la lista</a>
        </li>
        <li>
            <a href="{{ path('tipoasuntos_edit', { 'id': tipoasunto.idTipoasunto }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
